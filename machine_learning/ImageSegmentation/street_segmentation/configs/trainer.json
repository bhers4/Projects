{
  "name": "Image Segmentation",
  "dataset": {
    "name": "CIFAR10",
    "batch_size": 2048,
    "shuffle": true,
    "split": 0.8
  },
  "run": {
    "num_epochs": 12
  },
  "models": {
    "name": "Resnet18",
    "task": "classification",
    "num_classes": 10,
    "pretrained": true
  },
  "loss": {
    "name": "CrossEntropyLoss"
  },
  "optim": {
    "name": "Adam",
    "lr": 0.01
  }
}